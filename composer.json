{
    "name": "safiul0073/laravel-project-updater",
    "description": "A Laravel package to easily update your project by clicking a button on your dashboard",
    "keywords": [
        "laravel",
        "update",
        "database",
        "files",
        "production",
        "deployment"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Safiul",
            "email": "mdsafiul0073@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Src\\Providers\\PhpUpdateProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
} 